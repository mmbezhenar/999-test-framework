{
  "default": {
    "formatOptions": {
      "snippetInterface": "async-await"
    },
    "paths": [
      "src/features/"
    ],
    "dryRun": false,
    "require": [
      "src/step-definitions/api/pet-store/*.ts",
      "src/step-definitions/api/restful-booker/*.ts",
      "src/step-definitions/ui/youtube/*.ts",
      "src/step-definitions/ui/999/*.ts",
      "src/step-definitions/hooks/*.ts"
    ],
    "requireModule": [
      "ts-node/register"
    ],
    "format": [
      "html:cucumber-report.html"
    ],
    "retry": 3
  },
  "api": {
    "paths": [
      "src/features/pet-store/api.pet-store.feature",
      "src/features/restful-booker/api.login.feature"
    ],
    "require": [
      "src/step-definitions/api/pet-store/*.ts",
      "src/step-definitions/api/restful-booker/*.ts",
      "src/step-definitions/hooks/after-hook.api.ts",
      "src/step-definitions/hooks/before-hook.api.ts"
    ],
    "requireModule": [
      "ts-node/register"
    ]
  },
  "ui": {
    "paths": [

      "src/features/999/ui.user-profile.feature"
    ],
    "require": [
      "src/step-definitions/ui/999/*.ts",
      "src/step-definitions/ui/youtube/*.ts",
      "src/step-definitions/hooks/after-hook.ts",
      "src/step-definitions/hooks/before-hook.ts"
    ],
    "requireModule": [
      "ts-node/register"
    ],
    "retry" : 5
  }
}
